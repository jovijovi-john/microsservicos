{
  "name": "server-grpc-node",
  "version": "1.0.0",
  "description": "Servidor gRPC Node.js para ImageService",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "ts-node src/server.ts",
    "generate": "npm run generate:proto",
    "generate:proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:. --grpc_out=grpc_js:. -I../api_grpc ../api_grpc/image.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.10"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "grpc-tools": "^1.12.4",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
