{
  "name": "api-grpc-client",
  "version": "1.0.0",
  "description": "Cliente gRPC que consome os microsservi√ßos ImageService",
  "main": "dist/client.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/client.js",
    "dev": "ts-node src/client.ts",
    "generate": "npm run generate:proto",
    "generate:proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./src/generated -I. image.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.10",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "grpc-tools": "^1.12.4",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}

